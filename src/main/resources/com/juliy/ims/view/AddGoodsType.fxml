<?xml version="1.0" encoding="UTF-8"?>

<?import com.leewyatt.rxcontrols.controls.RXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0"
         prefWidth="900.0"
         styleClass="root" stylesheets="@../css/style.css"
         type="AnchorPane" xmlns="http://javafx.com/javafx/18"
         fx:controller="com.juliy.ims.controller.AddGoodsTypeController">
    <HBox layoutX="30.0" layoutY="92.0" AnchorPane.leftAnchor="30.0"
          AnchorPane.topAnchor="30.0">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="类别名称">
            <font>
                <Font name="System Bold" size="16.0"/>
            </font>
        </Text>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="*"/>
    </HBox>
    <RXTextField fx:id="tfGoodsTypeName" layoutX="30.0" layoutY="120.0"
                 styleClass="tf-add" AnchorPane.leftAnchor="30.0"
                 AnchorPane.topAnchor="55.0"/>
    <Text fx:id="txtTypeNameError" fill="RED" layoutX="202.0" layoutY="72.0"
          strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false"
          AnchorPane.leftAnchor="200.0">
        <font>
            <Font size="14.0"/>
        </font>
    </Text>
    <Button focusTraversable="false" layoutX="83.0" layoutY="555.0"
            mnemonicParsing="false" onAction="#addNewGoodsType"
            styleClass="btn-add" text="添加" AnchorPane.bottomAnchor="20.0"
            AnchorPane.leftAnchor="60.0"/>
    <HBox alignment="CENTER" layoutX="25.0" layoutY="521.0" prefHeight="18.0"
          prefWidth="150.0" AnchorPane.bottomAnchor="60.0">
        <Text fx:id="txtSuccess" fill="GREEN" strokeType="OUTSIDE"
              strokeWidth="0.0" text="text" visible="false">
            <font>
                <Font size="14.0"/>
            </font>
        </Text>
    </HBox>
</fx:root>
