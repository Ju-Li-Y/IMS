<?xml version="1.0" encoding="UTF-8"?>

<?import com.leewyatt.rxcontrols.controls.RXLineButton?>
<?import com.leewyatt.rxcontrols.controls.RXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0"
         prefWidth="900.0"
         styleClass="root" stylesheets="@../css/style.css"
         type="AnchorPane" xmlns="http://javafx.com/javafx/18"
         fx:controller="com.juliy.ims.controller.OutRecordController">
    <ScrollPane layoutX="84.0" layoutY="62.0" AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
        <VBox prefWidth="880.0" spacing="30.0">
            <padding>
                <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
            </padding>
            <AnchorPane>
                <AnchorPane AnchorPane.leftAnchor="0.0"
                            AnchorPane.topAnchor="0.0">
                    <HBox AnchorPane.bottomAnchor="25.0"
                          AnchorPane.leftAnchor="0.0"
                          AnchorPane.topAnchor="0.0">
                        <Text strokeType="OUTSIDE" strokeWidth="0.0"
                              text="单据类型">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Text>
                    </HBox>
                    <RXTextField fx:id="tfReceiptType" disable="true"
                                 layoutY="25.0" styleClass="tf-add"
                                 text="采购入库" AnchorPane.bottomAnchor="0.0"
                                 AnchorPane.leftAnchor="0.0"
                                 AnchorPane.topAnchor="25.0">
                        <font>
                            <Font name="System Bold" size="12.0"/>
                        </font>
                    </RXTextField>
                </AnchorPane>
                <AnchorPane layoutX="40.0" layoutY="40.0"
                            AnchorPane.leftAnchor="350.0"
                            AnchorPane.topAnchor="0.0">
                    <HBox AnchorPane.leftAnchor="0.0"
                          AnchorPane.topAnchor="0.0">
                        <Text strokeType="OUTSIDE" strokeWidth="0.0"
                              text="单据编号">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Text>
                    </HBox>
                    <RXTextField fx:id="tfReceiptId" disable="true"
                                 layoutY="25.0" styleClass="tf-add"
                                 text="自动生成，无需填写"
                                 AnchorPane.bottomAnchor="20.0"
                                 AnchorPane.leftAnchor="0.0"
                                 AnchorPane.topAnchor="25.0"/>
                </AnchorPane>
                <AnchorPane layoutX="30.0" layoutY="138.0"
                            AnchorPane.leftAnchor="0.0"
                            AnchorPane.topAnchor="90.0">
                    <HBox AnchorPane.bottomAnchor="25.0"
                          AnchorPane.leftAnchor="0.0"
                          AnchorPane.topAnchor="0.0">
                        <Text strokeType="OUTSIDE" strokeWidth="0.0"
                              text="客户">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Text>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="*">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Text>
                    </HBox>
                    <ComboBox fx:id="cbbCompany" prefWidth="200.0"
                              AnchorPane.bottomAnchor="20.0"
                              AnchorPane.leftAnchor="0.0"
                              AnchorPane.topAnchor="25.0">
                        <cursor>
                            <Cursor fx:constant="HAND"/>
                        </cursor>
                    </ComboBox>
                    <Text fx:id="txtCompanyError" fill="RED" layoutX="124.0"
                          layoutY="16.0" strokeType="OUTSIDE" strokeWidth="0.0"
                          text="Text" visible="false"
                          AnchorPane.bottomAnchor="0.0"
                          AnchorPane.leftAnchor="0.0">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </Text>
                </AnchorPane>
                <AnchorPane layoutX="366.0" layoutY="208.0"
                            AnchorPane.leftAnchor="350.0"
                            AnchorPane.topAnchor="90.0">
                    <HBox AnchorPane.bottomAnchor="25.0"
                          AnchorPane.leftAnchor="0.0"
                          AnchorPane.topAnchor="0.0">
                        <Text strokeType="OUTSIDE" strokeWidth="0.0"
                              text="出库仓库">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Text>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="*">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Text>
                    </HBox>
                    <ComboBox fx:id="cbbWhs" prefWidth="200.0"
                              AnchorPane.bottomAnchor="20.0"
                              AnchorPane.leftAnchor="0.0"
                              AnchorPane.topAnchor="25.0">
                        <cursor>
                            <Cursor fx:constant="HAND"/>
                        </cursor>
                    </ComboBox>
                    <Text fx:id="txtWhsError" fill="RED" layoutX="124.0"
                          layoutY="16.0" strokeType="OUTSIDE" strokeWidth="0.0"
                          text="Text" visible="false"
                          AnchorPane.bottomAnchor="0.0"
                          AnchorPane.leftAnchor="0.0">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </Text>
                </AnchorPane>
            </AnchorPane>
            <AnchorPane prefWidth="820.0">
                <HBox AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <Text strokeType="OUTSIDE" strokeWidth="0.0"
                          text="入库明细">
                        <font>
                            <Font name="System Bold" size="16.0"/>
                        </font>
                    </Text>
                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="*">
                        <font>
                            <Font name="System Bold" size="16.0"/>
                        </font>
                    </Text>
                </HBox>
                <RXLineButton layoutX="14.0" layoutY="69.0"
                              onAction="#addNewRecord" prefHeight="30.0"
                              prefWidth="60.0" text="+ 添加" textFill="#66cccc"
                              AnchorPane.bottomAnchor="0.0"
                              AnchorPane.leftAnchor="10.0">
                    <font>
                        <Font name="System Bold" size="14.0"/>
                    </font>
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                </RXLineButton>
                <TableView fx:id="table" fixedCellSize="30.0"
                           focusTraversable="false" minWidth="60.0"
                           prefHeight="55.0" prefWidth="820.0"
                           AnchorPane.bottomAnchor="30.0"
                           AnchorPane.leftAnchor="0.0"
                           AnchorPane.rightAnchor="0.0"
                           AnchorPane.topAnchor="25.0">
                    <columns>
                        <TableColumn fx:id="tcDelete" maxWidth="30.0"
                                     minWidth="30.0" prefWidth="30.0"
                                     resizable="false" sortable="false"/>
                        <TableColumn fx:id="tcGoodsId" minWidth="150.0"
                                     prefWidth="150.0" resizable="false"
                                     sortable="false" text="货品编号"/>
                        <TableColumn fx:id="tcGoodsType" maxWidth="300.0"
                                     minWidth="120.0" prefWidth="120.0"
                                     resizable="false" sortable="false"
                                     text="货品类别"/>
                        <TableColumn fx:id="tcGoodsName" maxWidth="300.0"
                                     minWidth="150.0" prefWidth="150.0"
                                     resizable="false" sortable="false"
                                     text="货品名称"/>
                        <TableColumn fx:id="tcGoodsSpec" maxWidth="300.0"
                                     minWidth="140.0" prefWidth="140.0"
                                     resizable="false" sortable="false"
                                     text="规格型号"/>
                        <TableColumn fx:id="tcSellPrice" maxWidth="150.0"
                                     minWidth="70.0" prefWidth="70.0"
                                     resizable="false" sortable="false"
                                     text="参考售价"/>
                        <TableColumn fx:id="tcOutQty" maxWidth="150.0"
                                     minWidth="70.0" prefWidth="70.0"
                                     resizable="false" sortable="false"
                                     text="出库数量"/>
                        <TableColumn fx:id="tcAmount" maxWidth="150.0"
                                     minWidth="70.0" prefWidth="70.0"
                                     resizable="false" sortable="false"
                                     text="金额"/>
                    </columns>
                </TableView>
                <Text fx:id="txtTableError" fill="RED" layoutX="92.0"
                      layoutY="101.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Text" visible="false" AnchorPane.bottomAnchor="5.0"
                      AnchorPane.leftAnchor="100.0">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Text>
            </AnchorPane>
            <AnchorPane>
                <AnchorPane AnchorPane.leftAnchor="0.0"
                            AnchorPane.topAnchor="0.0">
                    <HBox AnchorPane.leftAnchor="0.0"
                          AnchorPane.topAnchor="0.0">
                        <Text strokeType="OUTSIDE" strokeWidth="0.0"
                              text="合计金额">
                            <font>
                                <Font name="System Bold" size="16.0"/>
                            </font>
                        </Text>
                    </HBox>
                    <RXTextField fx:id="tfTotalAmount" disable="true"
                                 layoutY="25.0" styleClass="tf-add" text="0"
                                 AnchorPane.bottomAnchor="0.0"
                                 AnchorPane.leftAnchor="0.0"
                                 AnchorPane.topAnchor="25.0"/>
                </AnchorPane>
            </AnchorPane>
            <AnchorPane>
                <HBox alignment="CENTER" prefHeight="18.0" prefWidth="150.0">
                    <Text fx:id="txtSuccess" fill="GREEN" strokeType="OUTSIDE"
                          strokeWidth="0.0" text="text" visible="false">
                        <font>
                            <Font size="14.0"/>
                        </font>
                    </Text>
                </HBox>
                <Button focusTraversable="false" mnemonicParsing="false"
                        onAction="#createNewOutRecord" styleClass="btn-add"
                        text="提交" AnchorPane.leftAnchor="30.0"
                        AnchorPane.topAnchor="20.0"/>
            </AnchorPane>
        </VBox>
    </ScrollPane>
</fx:root>
